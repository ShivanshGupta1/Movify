{
    "name": "Movify",
    "description": "This small app let's you search for any movie, get its rating, plot and other details for you to know!",
    "version": "2.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html"
      },
    "icons": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
      }
      
  }